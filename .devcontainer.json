{
  "name": "Dev Container for LSTM music generator",
  "build": {
    "dockerfile": "./Dockerfile"
  },
  "settings": {
    "python.defaultInterpreterPath": "/usr/bin/python"
  },
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "EditorConfig.EditorConfig"
      ]
    }
  }
}
