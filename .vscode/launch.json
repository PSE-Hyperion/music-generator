{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run music_generation_lstm",
      "type": "debugpy",
      "request": "launch",
      "module": "music_generation_lstm.main",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src",
        "TF_ENABLE_ONEDNN_OPTS": "0",
        "TF_CPP_MIN_LOG_LEVEL": "2"
      }
    },
    {
      "name": "Run test",
      "type": "debugpy",
      "request": "launch",
      "module": "test.main",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src",
        "TF_ENABLE_ONEDNN_OPTS": "0",
        "TF_CPP_MIN_LOG_LEVEL": "2"
      }
    }
  ]
}